<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <local-check-config name="Blackpepper Basic" location="config/build/checkstyle.xml" type="project" description="">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <local-check-config name="BlackPepper Additional" location="config/build/checkstyle-additional.xml" type="project" description="">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="BP Basic" enabled="true" check-config-name="Blackpepper Basic" local="true">
        <file-match-pattern match-pattern=".java$" include-pattern="true"/>
    </fileset>
    <fileset name="BP Additional" enabled="true" check-config-name="BlackPepper Additional" local="true">
        <file-match-pattern match-pattern=".java$" include-pattern="true"/>
    </fileset>
</fileset-config>
